<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.krzysiek.phonebase">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

        <activity android:name=".PhoneSelect"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AppInfo"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".ShowPhone"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".BrandListAct"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".MultiplePhoneSelect"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Show2Phones"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Show3Phones"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Show4Phones"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Assisstant"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".AssisstantSuggestions"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>